<div class="grid_8 chapter">
  <div>
  <% name = truncate chapter.name, :length => 18 %>
  <h3><%= link_to name, chapter_path(chapter) %></h3>
  <span class="info">
    <%= truncate chapter.geographic_territory.parent.name, :length => 35 %><br />
    <%= pluralize chapter.users_count, 'Member' %>
  </span>
  </div>
</div>
